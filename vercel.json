{
    "version": 2,
    "builds": [
      {
        "src": "api/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/events",
        "dest": "/api/server.js"
      },
      {
        "src": "/",
        "dest": "/public/index.html"
      }

      {
        "src": "/main.js",
        "dest": "/public/main.js"
      }
    ]
  }